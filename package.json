    "pump": "3.0.0",
    "tldjs": "2.3.1"
  },
  "devDependencies": {
    "mocha": "5.1.1",
    "node-dev": "3.1.3",
    "supertest": "3.1.0",
    "ws": "5.1.1"
  },
  "scripts": {
    "test": "mocha --check-leaks --require esm './**/*.test.js'",
