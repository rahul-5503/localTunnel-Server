  },
  "dependencies": {
    "book": "1.3.3",
    "debug": "3.1.0",
    "esm": "3.0.34",
    "http-proxy": "1.18.1",
    "human-readable-ids": "1.0.3",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.2.0",
    "koa": "2.5.1",
    "koa-jwt": "4.0.4",
